{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "#{ConnectionStrings__DefaultConnection}"
  },
  "JWT": {
    "Key": "#{JWT__Key}",
    "Issuer": "#{JWT__Issuer}",
    "Audience": "#{JWT__Audience}"
  }
}
